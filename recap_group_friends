function analyzeFriendGroups(group1, group2) {
    let set1 = new Set(group1);
    let set2 = new Set(group2);
    // Write your code here
    let mutualFriends = 0;
    let exclusiveToFirst = 0;
    
    

    set1.forEach((n)=>{
        if (set2.has(n)){
            mutualFriends += 1;
        }
        else{
            exclusiveToFirst += 1;
        }
    })
    let exclusiveToSecond = set2.size - mutualFriends;
    let potentialConnections = exclusiveToSecond * exclusiveToFirst;
    let isSubset = mutualFriends == set1.size || mutualFriends == set2.size;

    
    return {
        mutualFriends: mutualFriends,
        exclusiveToFirst: exclusiveToFirst,
        exclusiveToSecond: exclusiveToSecond,
        potentialConnections: potentialConnections,
        isSubset: isSubset
    }
}
